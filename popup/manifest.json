{
  "author": "守夜人",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "description": "一款简约简洁的新标签页",
  "icons": {
    "128": "assets/icons/favicon.ico",
    "16": "assets/icons/favicon.ico",
    "48": "assets/icons/favicon.ico"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Night Watch",
  "version": "2.0.1",
  "permissions": [ "tabs", "storage", "background" ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "assets/icons/favicon.ico",
    "default_title": "Popup",
    "default_popup": "popup/popup.html"
  }
}
